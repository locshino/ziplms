{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "asmit/resized-column": "^1.1",
        "bezhansalleh/filament-language-switch": "^4.0",
        "bezhansalleh/filament-shield": "^4.0",
        "cmsmaxinc/filament-error-pages": "^2.0",
        "devonab/filament-easy-footer": "^2.1@beta",
        "dutchcodingcompany/filament-developer-logins": "^2.0",
        "dutchcodingcompany/filament-socialite": "^3.0",
        "eightynine/filament-excel-import": "^4.0",
        "encoredigitalgroup/filament-helpers": "^4.0@RC",
        "filafly/brisk": "^1.0",
        "filament/filament": "~4.0",
        "filament/spatie-laravel-media-library-plugin": "^4.0",
        "filament/spatie-laravel-tags-plugin": "^4.0",
        "gboquizosanchez/filament-log-viewer": "^2.0",
        "geoip2/geoip2": "^3.2",
        "guava/calendar": "^2.0",
        "guava/filament-icons": "^1.0",
        "guava/filament-modal-relation-managers": "^2.0",
        "jeffersongoncalves/filament-cookie-consent": "^2.0",
        "laravel/framework": "^12.0",
        "laravel/octane": "^2.12",
        "laravel/reverb": "^1.0",
        "laravel/telescope": "^5.11",
        "laravel/tinker": "^2.10.1",
        "leandrocfe/filament-apex-charts": "~4.0",
        "malzariey/filament-daterangepicker-filter": "^4.0",
        "malzariey/filament-lexical-editor": "^2.0",
        "marcelweidum/filament-expiration-notice": "^1.0",
        "marjose123/filament-lockscreen": "*",
        "martinpetricko/filament-restore-or-create": "^2.0",
        "novadaemon/filament-pretty-json": "^3.1",
        "owen-it/laravel-auditing": "^14.0",
        "owenvoke/blade-fontawesome": "^2.9",
        "predis/predis": "^3.2",
        "pxlrbt/filament-activity-log": "^2.0",
        "pxlrbt/filament-environment-indicator": "^3.1",
        "pxlrbt/filament-excel": "^3.1",
        "pxlrbt/filament-spotlight": "^2.0",
        "rappasoft/laravel-authentication-log": "^5.0",
        "schmeits/filament-character-counter": "^4.0",
        "spatie/laravel-activitylog": "^4.10",
        "spatie/laravel-medialibrary": "^11.13",
        "spatie/laravel-permission": "^6.21",
        "spatie/laravel-sluggable": "^3.7",
        "spatie/laravel-tags": "^4.10",
        "stechstudio/filament-impersonate": "^4.0",
        "stephenjude/filament-two-factor-authentication": "^3.0",
        "swisnl/filament-backgrounds": "^2.0",
        "tapp/filament-auditing": "^4.0",
        "tapp/filament-authentication-log": "~5.0",
        "tapp/filament-maillog": "^2.0",
        "torann/geoip": "^3.0",
        "yebor974/filament-renew-password": "^3.0",
        "zpmlabs/filament-api-docs-builder": "^2.0",
        "zvizvi/relation-manager-repeater": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.6",
        "fakerphp/faker": "^1.23",
        "laravel/boost": "^1.0",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php artisan config:clear",
            "@php artisan clear-compiled",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"npm run dev\" \"php artisan reverb:start --debug\" --names='server,queue,vite,reverb'"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
