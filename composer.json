{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "locshino/ziplms",
    "type": "project",
    "description": "The official Laravel starter kit for Livewire.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "afsakar/filament-otp-login": "^1.4",
        "bezhansalleh/filament-shield": "^3.3",
        "erag/laravel-pwa": "^1.7",
        "filament/filament": "^3.3",
        "filament/spatie-laravel-media-library-plugin": "^3.3",
        "filament/spatie-laravel-settings-plugin": "^3.3",
        "filament/spatie-laravel-tags-plugin": "^3.3",
        "filament/spatie-laravel-translatable-plugin": "^3.3",
        "laravel/framework": "^12.0",
        "laravel/octane": "^2.9",
        "laravel/tinker": "^2.10.1",
        "livewire/flux": "*",
        "livewire/livewire": "^3.6",
        "livewire/volt": "^1.7",
        "maatwebsite/excel": "^3.1",
        "outerweb/filament-translatable-fields": "^2.1",
        "pxlrbt/filament-activity-log": "^1.1",
        "pxlrbt/filament-excel": "^2.4",
        "spatie/laravel-activitylog": "^4.10",
        "spatie/laravel-backup": "^9.3",
        "spatie/laravel-data": "^4.15",
        "spatie/laravel-health": "^1.34",
        "spatie/laravel-medialibrary": "^11.13",
        "spatie/laravel-model-states": "^2.11",
        "spatie/laravel-one-time-passwords": "^1.0",
        "spatie/laravel-permission": "^6.19",
        "spatie/laravel-settings": "^3.4",
        "spatie/laravel-tags": "^4.10",
        "spatie/laravel-translatable": "^6.11"
    },
    "require-dev": {
        "amnuts/opcache-gui": "*",
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.22",
        "laravel/sail": "^1.43",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"npm run dev\" --names='server,queue,vite'"
        ],
        "optimize": [
            "@php artisan optimize:clear",
            "@php artisan optimize",
            "@php artisan filament:optimize-clear",
            "@php artisan filament:optimize"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
